{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "typeRoots" : ["./netlify/functions/common/types", "./node_modules/@types"]
    },
    "include": ["netlify/functions/**/*.ts"],
    "exclude": ["node_modules"]
  }